<%- include('blocks/header', { bot, user, title: `${tag} Bots | Discord Bot House`, embedding: { default: true } }) %>

<meta name="description" content="Discover bots with the tag <%= tag %>!">
<meta name="og:title" property="og:title" content="<%= tag %> bots | Discord Bot House">
<meta name="og:description" property="og:description" content="Discover bots with the tag <%= tag %>!">
<meta name="twitter:card" content="Discover bots with the tag <%= tag %>!">

<div class="container1 main-bkg-a" style="margin-left: 3%; margin-right: 3%;margin-top: 5px;margin-bottom: 20px;box-shadow:0 8px 16px 0 rgba(0, 0, 0, 1.2); color: #fff; background-color: #1d1e21;">
<h2 style="padding-top: 20px; margin: 2%; color:white"><i class="fas fa-tag"></i>  Bots tagged with <%= tag %> Tag:</h2>

<div style="padding: 35px 20px 20px 35px">
<div class="row">
  <% if (featuredBots.length == 0){ %>
    <h2 style="color: #fff;padding-top: 0px;padding-bottom: 0px;"><i class="fas fa-walking"></i> No bots have been found.</h2>
 <% } %>
  <% featuredBots.forEach(theBot => { if (!bot.users.get(theBot.id)) bot.users.get(theBot.id) = { presence: { status: "offline" }, username: theBot.name  } %>
    <%- include("blocks/card.ejs", { theBot, bot }); %>
  <% }); %>
</div>
<center><nav aria-label="...">
  <ul class="pagination pagination-lg">
    <% for (var i = 0; i < numberOfPages; i++) { %>
      <li class="page-item"><a class="page-link" href="/tag/<%= tag %>?page=<%= i + 1 %>"><%= i + 1 %></a></li>
     <% } %>
  </ul>
</nav></center>
</div>
</div>

