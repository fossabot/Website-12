<section  style="background-color:#2C2F33;min-height:250px; margin-top: -20px;  box-shadow: 5px 5px 8px rgb(0, 0, 0); width: 100%!important;" id="intro" class="section-intro hero-bkg-animated">
  <div class="overlay">
    <div class="container">
      <div class="main-text" >
          <h1 class="intro-title" style="font-size:4vw;" >Welcome To <span style="color: #7289da"> <bold>Discord Bot World</bold> </span></h1>
      </div>
         <form method="get" action="/search" class="inline-form search-form">
            <input type="search" name="name" placeholder="Search" class="search-input">
            <button type="submit" class="search-button">
              <svg class="submit-button">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#search"></use>
              </svg>
            </button>
          </form>
      <svg xmlns="http://www.w3.org/2000/svg" width="0" height="0" display="none">
        <symbol id="search" viewBox="0 0 32 32">
          <path d="M 19.5 3 C 14.26514 3 10 7.2651394 10 12.5 C 10 14.749977 10.810825 16.807458 12.125 18.4375 L 3.28125 27.28125 L 4.71875 28.71875 L 13.5625 19.875 C 15.192542 21.189175 17.250023 22 19.5 22 C 24.73486 22 29 17.73486 29 12.5 C 29 7.2651394 24.73486 3 19.5 3 z M 19.5 5 C 23.65398 5 27 8.3460198 27 12.5 C 27 16.65398 23.65398 20 19.5 20 C 15.34602 20 12 16.65398 12 12.5 C 12 8.3460198 15.34602 5 19.5 5 z" />
        </symbol>
      </svg>
    </div>
  </div>
  <center>
    <div class="tag" style="width:70%">
      <% tags = ['Roleplay','Social','Anime','Music','Moderation','Logging','Web Dashboard','Utility','Leveling','Statistics','Role Management','Fun','Game','Meme','Soundboard']
        function shuffle(a) {
          for (let i = a.length - 1; i > 0; i--) {
              const j = Math.floor(Math.random() * (i + 1));
              [a[i], a[j]] = [a[j], a[i]];
          }
          return a;
        }
       tags = shuffle(tags)
        for (var i = 0; i < tags.length; i++) {
          var tag = tags[i]
          let label
          const redtags = ['Roleplay','Social','Anime','Music']
          const bluetags = ['Moderation','Logging','Web Dashboard','Utility']
          const graytags = ['Leveling','Statistics','Role Management']
          const greentags = ['Fun','Game','Meme','Soundboard']
          if(redtags.includes(tag)) label = "danger"
          if(bluetags.includes(tag)) label = "primary"
          if(graytags.includes(tag)) label = "default"
          if(greentags.includes(tag)) label = "success" %>
      <span onclick="window.location.href ='/tag/<%=tag%>';"style="cursor: pointer;" class="label label-<%=label%>"><%= tag %></span>
      <%  }; %>
    </div>
  </center>
</section>

<div class="container1  main-bkg" style="margin-left: 3%; margin-right: 3%;margin-top: 5px;margin-bottom: 10px;box-shadow:0 8px 16px 0 rgba(0, 0, 0, 1.2);">
  <h2 style="padding-top: 20px; margin-left: 3%;color:#fff"><i class="fas fa-star"></i> Featured Bots:</h2>

<div style="padding: 35px 20px 20px 35px">
<div class="row">
  <% featuredBots.forEach(theBot => { if (!bot.users.get(theBot.id)) bot.users.get(theBot.id) = { presence: { status: "offline" }, username: theBot.name  } %>
    <%- include("blocks/card.ejs", { theBot, bot }); %>
  <% }); %>
</div>
</div>
</div>
